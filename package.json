{
  "name": "project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm ci && tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cap:build": "npm run build && npx cap sync",
    "cap:ios": "npm run cap:build && npx cap open ios",
    "cap:android": "npm run cap:build && npx cap open android",
    "cap:sync": "npx cap sync",
    "cap:serve": "npm run build && npx cap run ios --livereload --external",
    "cap:serve:android": "npm run build && npx cap run android --livereload --external",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@capacitor/app": "^5.0.6",
    "@capacitor/camera": "^5.0.7",
    "@capacitor/core": "^5.5.1",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/local-notifications": "^5.0.6",
    "@capacitor/push-notifications": "^5.1.0",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@capacitor/toast": "^5.0.6",
    "@react-google-maps/api": "^2.19.2",
    "@supabase/auth-ui-react": "^0.4.6",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.39.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "expo": "^53.0.13",
    "expo-status-bar": "~2.2.3",
    "ical.js": "^1.5.0",
    "lucide-react": "^0.292.0",
    "luxon": "^3.4.4",
    "pkce-challenge": "^3.1.0",
    "query-string": "^8.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-web": "^0.20.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@capacitor/android": "^5.7.8",
    "@capacitor/cli": "^5.5.1",
    "@types/luxon": "^3.4.2",
    "@types/react": "~19.0.10",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
